[
	{
		"activePorts": [80, 8080],
		"staticServing": [
			{
				"webAddress": "/",
				"aliases": ["/home", "/index", "/home.html", "/index.html"],
				"localResponseFile": "static/sample_page.html"
			}
		],
		"apiLogic": [
			{
				"webApiAddress": "/api/v1/media/image",
				"aliases": ["/api/media/image"],
				"logicHandler": "./src/api/v1/media/imageSubmit.js",
				"acceptedMethod": "POST",
				"acceptedPayload": [
					"verToken",
					"userID",
					"imageType",
					"image"
				]
			},
			{
				"webApiAddress": "/api/v1/media/image",
				"aliases": ["/image", "/api/media/image"],
				"logicHandler": "./src/api/media/imageFetch.js",
				"acceptedMethod": "GET",
				"acceptedPayload": [
					"h" // `/image?h=912EC803B2CE49E4A541068D495AB570`
				]
			},
			{
				"webApiAddress": "/api/v1/media/list",
				"aliases": ["/api/media/list"],
				"logicHandler": "./src/api/v1/media/list.js",
				"acceptedMethod": "GET",
				"acceptedPayload": [
					"count"
				]
			}
		]
	}
]
